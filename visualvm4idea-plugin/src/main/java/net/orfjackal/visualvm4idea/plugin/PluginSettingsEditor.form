<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="net.orfjackal.visualvm4idea.plugin.PluginSettingsEditor">
  <grid id="27dc6" binding="rootComponent" layout-manager="FormLayout">
    <rowspec value="center:d:noGrow"/>
    <colspec value="fill:d:grow"/>
    <constraints>
      <xy x="20" y="20" width="505" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="20d6e" layout-manager="FormLayout">
        <rowspec value="center:p:grow"/>
        <rowspec value="center:d:noGrow"/>
        <rowspec value="center:max(d;4px):noGrow"/>
        <rowspec value="center:max(d;4px):noGrow"/>
        <colspec value="fill:12px:noGrow"/>
        <colspec value="fill:max(d;4px):noGrow"/>
        <colspec value="fill:max(d;120dlu):grow"/>
        <colspec value="fill:max(d;4px):noGrow"/>
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties/>
        <border type="empty" title="VisualVM home directory:"/>
        <children>
          <component id="6d011" class="javax.swing.JRadioButton" binding="customHomeRadioButton">
            <constraints>
              <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <forms/>
            </constraints>
            <properties>
              <selected value="true"/>
              <text value=""/>
            </properties>
          </component>
          <component id="c40f2" class="javax.swing.JTextField" binding="customHomeField">
            <constraints>
              <grid row="2" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="1" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <forms defaultalign-horz="false"/>
            </constraints>
            <properties/>
          </component>
          <component id="eec64" class="javax.swing.JButton" binding="customHomeBrowse">
            <constraints>
              <grid row="2" column="3" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
              <forms/>
